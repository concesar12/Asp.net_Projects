@model ListModel 
@*This will make strongly yped view*@

<div class="list-container">
    @{
        //ViewBag.ListTitle = "Updated";
    }
    @*<h3>@ViewBag.ListTitle</h3> instead of this I can use the one below*@
    <h3>@Model.ListTitle</h3>
    <ul class="list">
        @foreach (string barrio in Model.ListItems)
        {
            <li>@barrio</li>
        }
    </ul>
</div>